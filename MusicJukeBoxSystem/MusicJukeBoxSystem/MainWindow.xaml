        mc:Ignorable="d"
        Title="MainWindow" Height="Auto" Width="Auto" d:DesignWidth="944" d:DesignHeight="537"
        SizeToContent="WidthAndHeight">
    <Grid MinWidth="450" MinHeight="250">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid HorizontalAlignment="Left" Height="313" Margin="0,101,0,0" VerticalAlignment="Top" Width="220">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button Content="Login" HorizontalAlignment="Left" Margin="70,88,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
            <Button Content="Registration" HorizontalAlignment="Left" Margin="70,138,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.027,0.6" Click="Button_Click_1"/>
            <Button Content="Search" HorizontalAlignment="Left" Margin="70,190,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.133,0.7"/>
