        <Grid HorizontalAlignment="Left" Width="567" Height="316" VerticalAlignment="Top" Margin="225,101,0,0">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <StackPanel Name="main" Width="Auto" Height="Auto">
            </StackPanel>
        </Grid>
        <Grid HorizontalAlignment="Left" Height="101" VerticalAlignment="Top" Width="792">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Image HorizontalAlignment="Left" Height="81" Margin="36,10,0,0" VerticalAlignment="Top" Width="88"/>
        </Grid>
    </Grid>
</Window>
